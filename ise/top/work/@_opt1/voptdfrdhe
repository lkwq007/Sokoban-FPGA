library verilog;
use verilog.vl_types.all;
entity X_OBUFT is
    generic(
        LOC             : string  := " UNPLACED";
        CAPACITANCE     : string  := "DONT_CARE";
        DRIVE           : integer := 12;
        IOSTANDARD      : string  := "DEFAULT";
        SLEW            : string  := "SLOW"
    );
    port(
        O               : out    vl_logic;
        CTL             : in     vl_logic;
        I               : in     vl_logic
    );
end X_OBUFT;
